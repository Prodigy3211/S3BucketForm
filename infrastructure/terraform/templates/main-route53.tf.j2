resource "aws_route53_record" "{{ env }}-record" {
    zone_id = "{{ zone_id }}"
    name    = "{{ domain_name }}"
    type    = "A" 
  
    alias {
      name                   = module.alb.alb_dns
      zone_id                = module.alb.alb_zone_id
      evaluate_target_health = true
    }
  }
  